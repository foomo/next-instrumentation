{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.9",
	"type": "module",
	"files": [
		"dist",
		"LICENSE"
	],
	"module": "./dist/client/index.js",
	"types": "./dist/client/index.d.ts",
	"exports": {
		".": {
			"edge": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			},
			"edge-light": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			},
			"browser": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			},
			"worker": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			},
			"workerd": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			},
			"import": {
				"types": "./dist/server/index.d.ts",
				"default": "./dist/server/index.js"
			},
			"node": {
				"types": "./dist/server/index.d.ts",
				"default": "./dist/server/index.js"
			},
			"default": "./dist/server/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"homepage": "https://github.com/foomo/next-instrumentation#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/foomo/next-instrumentation.git"
	},
	"bugs": {
		"url": "https://github.com/foomo/next-instrumentation/issues"
	},
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"lint:fix:unsafe": "biome check --write --unsafe ./",
		"prepublishOnly": "bun run lint && bun run type-check && bun run test && bun run build",
		"prereleaseOnly": "bun run lint && bun run type-check && bun run test && bun run build",
		"release": "bumpp",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"type-check": "tsc --noEmit"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@grafana/faro-web-tracing": "1.19.0",
		"@grafana/faro-web-sdk": "1.19.0",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"devDependencies": {
		"@types/bun": "1.3.1",
		"bumpp": "10.3.1",
		"bunup": "0.15.12",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"@types/react": ">=18",
		"@types/node": ">=20",
		"typescript": ">=4.5.0",
		"react": ">=18"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/node": {
			"optional": true
		},
		"typescript": {
			"optional": true
		},
		"react": {
			"optional": true
		}
	}
}
