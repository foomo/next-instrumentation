{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.5",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/foomo/next-instrumentation.git"
	},
	"bugs": {
		"url": "https://github.com/foomo/next-instrumentation/issues"
	},
	"homepage": "https://github.com/foomo/next-instrumentation#readme",
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./faro": {
			"types": "./dist/faro.d.ts",
			"import": "./dist/faro.js",
			"require": "./dist/faro.cjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "bun run clean && bun run build:esm && bun run build:types",
		"build:esm": "bun build ./src/index.ts ./src/faro.ts --outdir=./dist --format=esm --packages=external --target=node --splitting",
		"build:cjs": "bun build ./src/index.ts ./src/faro.ts --outdir=./dist --format=cjs --packages=external --target=node --outfile-extension=.cjs=.cjs",
		"build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
		"clean": "rm -rf dist",
		"lint": "biome check ./",
		"lint:fix": "biome check --write ./",
		"lint:fix:unsafe": "biome check --write --unsafe ./",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "bun run lint && bun run type-check && bun run build"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@grafana/faro-web-tracing": "1.19.0",
		"@grafana/faro-web-sdk": "1.19.0",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"devDependencies": {
		"bun-types": "1.3.1",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"@types/react": ">=18",
		"@types/node": ">=20",
		"react": ">=18"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"packageManager": "bun@1.3.1"
}
