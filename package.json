{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.6",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/foomo/next-instrumentation.git"
	},
	"bugs": {
		"url": "https://github.com/foomo/next-instrumentation/issues"
	},
	"homepage": "https://github.com/foomo/next-instrumentation#readme",
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		"./client": {
			"types": "./dist/client/index.d.ts",
			"import": "./dist/client/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"import": "./dist/server/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "bun run clean && bun run build:client && bun run build:server && bun run build:types",
		"build:client": "bun build ./src/client/index.ts --outdir=./dist/client --format=esm --packages=external --sourcemap=linked --target=browser",
		"build:server": "bun build ./src/server/index.ts --outdir=./dist/server --format=esm --packages=external --sourcemap=linked --target=node",
		"build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
		"clean": "rm -rf ./dist",
		"lint": "biome check ./",
		"lint:fix": "biome check --write ./",
		"lint:fix:unsafe": "biome check --write --unsafe ./",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "bun run lint && bun run type-check && bun run build"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@grafana/faro-web-tracing": "1.19.0",
		"@grafana/faro-web-sdk": "1.19.0",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"devDependencies": {
		"bun-types": "1.3.1",
		"typescript": "5.9.3"
	},
	"peerDependencies": {
		"@types/react": ">=18",
		"@types/node": ">=20",
		"react": ">=18"
	},
	"engines": {
		"node": ">=22.0.0"
	},
	"packageManager": "bun@1.3.1"
}
