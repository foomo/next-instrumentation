{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.1",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"scripts": {
		"build:clean": "rm -rf ./dist",
		"build:types": "tsc --emitDeclarationOnly",
		"build:cjs": "bun build src/index.ts --outdir=dist/cjs --sourcemap=linked --format=cjs --target=node --packages=external --minify",
		"build:esm": "bun build src/index.ts --outdir=dist/esm --sourcemap=linked --format=esm --target=node --packages=external --minify",
		"build": "bun run build:clean &&bun run build:types && bun run build:cjs && bun run build:esm",
		"lint": "biome check ./",
		"lint:fix": "biome check --write ./",
		"lint:fix:unsafe": "biome check --write --unsafe ./",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "bun run lint && bun run type-check && bun run build"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@pyroscope/nodejs": "0.4.7",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"peerDependenciesMeta": {
		"@types/node": {
			"optional": true
		},
		"typescript": {
			"optional": true
		}
	},
	"packageManager": "bun@1.3.1"
}
