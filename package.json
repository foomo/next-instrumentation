{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.12",
	"license": "MIT",
	"homepage": "https://github.com/foomo/next-instrumentation#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/foomo/next-instrumentation.git"
	},
	"bugs": {
		"url": "https://github.com/foomo/next-instrumentation/issues"
	},
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"files": [
		"dist",
		"LICENSE"
	],
	"exports": {
		"./client": {
			"import": {
				"types": "./dist/client/index.d.ts",
				"default": "./dist/client/index.js"
			}
		},
		"./server": {
			"import": {
				"types": "./dist/server/index.d.ts",
				"default": "./dist/server/index.js"
			}
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@grafana/faro-web-tracing": "1.19.0",
		"@grafana/faro-web-sdk": "1.19.0",
		"@grafana/faro-core": "1.19.0",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"devDependencies": {
		"@types/bun": "1.3.1"
	},
	"peerDependencies": {
		"@types/react": ">=18",
		"@types/node": ">=20",
		"react": ">=18"
	},
	"peerDependenciesMeta": {
		"@types/react": {
			"optional": true
		},
		"@types/node": {
			"optional": true
		},
		"react": {
			"optional": true
		}
	}
}
