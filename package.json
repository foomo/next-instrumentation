{
	"name": "@foomo/next-instrumentation",
	"version": "0.1.3",
	"license": "MIT",
	"keywords": [
		"foomo",
		"next"
	],
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"dist"
	],
	"type": "module",
	"module": "distindex.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.js"
		}
	},
	"scripts": {
		"build:clean": "rm -rf ./dist",
		"build:esm": "tsc",
		"build": "bun run build:clean && bun run build:esm",
		"lint": "biome check ./",
		"lint:fix": "biome check --write ./",
		"lint:fix:unsafe": "biome check --write --unsafe ./",
		"type-check": "tsc --noEmit",
		"prepublishOnly": "bun run lint && bun run type-check && bun run build"
	},
	"dependencies": {
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/instrumentation-http": "0.207.0",
		"@opentelemetry/exporter-prometheus": "0.207.0",
		"@opentelemetry/exporter-trace-otlp-grpc": "0.207.0",
		"@opentelemetry/semantic-conventions": "1.37.0",
		"@opentelemetry/host-metrics": "0.36.2",
		"@opentelemetry/sdk-metrics": "2.2.0",
		"@opentelemetry/sdk-node": "0.207.0",
		"@opentelemetry/sdk-trace-base": "2.2.0",
		"@opentelemetry/resources": "2.2.0",
		"@grafana/faro-web-tracing": "1.19.0",
		"@grafana/faro-web-sdk": "1.19.0",
		"pino": "9.14.0",
		"pino-http": "10.5.0",
		"@nestjs-labs/pino-http-extra": "1.5.0",
		"next-logger": "5.0.2"
	},
	"peerDependencies": {
		"@types/react": ">=18",
		"@types/node": ">=20",
		"react": ">=18",
		"typescript": ">=5"
	},
	"packageManager": "bun@1.3.1"
}
